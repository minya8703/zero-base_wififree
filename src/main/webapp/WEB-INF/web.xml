<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>와이파이 정보 구하기</display-name>
    <servlet>
        <servlet-name>mainPage</servlet-name>
        <servlet-class>com.example.zerobase_wififree.MainPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainPage</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>loadwifi</servlet-name>
        <servlet-class>com.example.zerobase_wififree.controller.LoadWifiController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loadwifi</servlet-name>
        <url-pattern>/loadwifi</url-pattern>
    </servlet-mapping>
</web-app>